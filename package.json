{
  "name": "psiquiapp",
  "version": "1.0.0",
  "description": "Sistema de gestión psiquiátrica - PsiquiApp",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "demo": "node test-server.js",
    "test": "jest --runInBand",
    "test:api": "jest tests/api --runInBand",
    "test:e2e": "playwright test",
    "test:smoke": "jest tests/smoke --runInBand",
    "test:all": "pnpm run test:smoke && pnpm run test:api",
    "seed": "node scripts/seed-database.js",
    "setup": "pnpm install && pnpm run seed",
    "audit": "pnpm audit"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": [
    "psiquiatria",
    "healthcare",
    "medical",
    "express",
    "nodejs"
  ],
  "author": "PsiquiApp Team",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.17.3",
    "mysql2": "^3.6.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "axe-core": "^4.8.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "playwright": "^1.55.0",
    "supertest": "^6.3.3"
  }
}
